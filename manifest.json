{
   "manifest_version": 3,
	"name": "blockSites",
	"author": "Kezia Sharnoff",
	"version": "1.0.0",
	"description": "Block groups of websites during work periods",
	"action": {
		"default_popup": "popup.html"
	},
	"icons": {
    "16": "icon.png",
    "32": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
   },
   "background": {
   	"service_worker": "background.js",
   	"type": "module"
   },
	"permissions": [
		"storage", 
		"tabs",
		"alarms" 
	]
}